{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap":true,
    "module": "esnext",
    "target": "es2016",
    "noUncheckedIndexedAccess": true,
    "strict": false,
    "jsx": "preserve",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@vue/*":["packages/*/src"]
    },
    "moduleResolution": "bundler",
  }
}
